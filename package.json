    "@trpc/server": "^10.37.1",
    "next": "^13.4.19",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "^1.13.1",
    "zod": "^3.22.3"
  },
  "devDependencies": {
    "@types/eslint": "^8.44.2",
    "@types/node": "^20.8.2",
    "@types/react": "^18.2.20",
